{
  "name": "qonico-pi",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "docker:build": "docker build -t qonico-pi .",
    "docker:run": "docker run -d -p 3001/tcp qonico-pi",
    "docker:urls:mac": "docker inspect --format 'http://localhost:{{(index (index .NetworkSettings.Ports \"3001/tcp\") 0).HostPort}}' $(docker ps -a -q)",
    "docker:urls:linux": "docker inspect --format 'http://{{ .NetworkSettings.IPAddress }}:{{(index (index .NetworkSettings.Ports \"3001/tcp\") 0).HostPort}}' $(docker ps -a -q)",
    "docker:stop:all": "docker stop $(docker ps -a -q)",
    "docker:rm:all": "docker rm $(docker ps -a -q)",
    "docker:logs": "docker logs -f $(docker ps -a -q)",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Fernando Caama√±o <fcaamano@qonico.com>",
  "license": "ISC",
  "dependencies": {
    "@tendermint/sig": "^0.4.1",
    "axios": "^0.19.2",
    "bip39": "^3.0.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "node-machine-id": "^1.1.12"
  },
  "devDependencies": {
    "@types/bip39": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/crypto-js": "^3.1.46",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/lodash": "^4.14.151",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  }
}
